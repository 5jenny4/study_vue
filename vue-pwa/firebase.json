{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "firebaseConfig.js"
    ],
    "rewrites": [
      {
        "source" : "**",
        "destination" : "/index.html"
      }
    ]
  }
}
